{
  "name": "terra-graphs",
  "private": true,
  "version": "0.1.0",
  "description": "terra-graphs",
  "engines": {
    "node": ">=8.9.2 <12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-graphs.git"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Graphs",
    "terra-graphs"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-graphs/issues"
  },
  "browserslist": [
    "extends @cerner/browserslist-config-terra"
  ],
  "eslintConfig": {
    "extends": "@cerner/terra",
    "overrides": [
      {
        "files": [
          "**/*/terra-dev-site/**/*.jsx"
        ],
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "import/no-unresolved": "off",
          "import/extensions": "off"
        }
      },
      {
        "files": [
          "*.doc.jsx"
        ],
        "rules": {
          "import/no-webpack-loader-syntax": "off",
          "import/first": "off",
          "import/no-duplicates": "off"
        }
      },
      {
        "files": [
          "*.js"
        ],
        "rules": {
          "import/no-cycle": "off",
          "no-param-reassign": "off",
          "template-curly-spacing": "off",
          "indent": [
            "error",
            2,
            {
              "ignoredNodes": [
                "TemplateLiteral"
              ]
            }
          ],
          "strict": "off",
          "func-names": "off",
          "no-undef": "off",
          "no-unused-expressions": "off",
          "no-new": "off",
          "no-underscore-dangle": "off",
          "no-console": "off"
        },
        "env": {
          "jasmine": true
        }
      }
    ],
    "rules": {
      "react/forbid-prop-types": "off"
    },
    "parser": "babel-eslint",
    "settings": {
      "polyfills": [
        "Promise",
        "Map"
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-terra",
    "rules": {
      "scss/at-mixin-pattern": "^(terra-)[a-z]+([a-z0-9-]+[a-z0-9]+)?$"
    }
  },
  "dependencies": {
    "d3": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@cerner/browserslist-config-terra": "^3.1.0",
    "@cerner/eslint-config-terra": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-minify-replace": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "check-installed-dependencies": "^1.0.1",
    "classnames": "^2.2.6",
    "commander": "^6.2.0",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "danger": "^10.5.0",
    "docdash": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "jsdoc": "^3.6.6",
    "lerna": "^3.22.1",
    "link-parent-bin": "^2.0.0",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.2.1",
    "one-cerner-style-icons": "github:cerner/one-cerner-style-icons",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-terra": "^3.7.0",
    "terra-aggregate-translations": "^1.9.0",
    "terra-dev-site": "^6.31.0",
    "terra-disclosure-manager": "^4.36.0",
    "terra-enzyme-intl": "^3.4.0",
    "terra-theme-context": "^1.4.0",
    "terra-toolkit": "^6.11.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "scripts": {
    "build": "webpack --config ./packages/carbon-graphs/webpack/webpack.config.js --env.TYPE=LIB",
    "build:dist": "webpack --config ./packages/carbon-graphs/webpack/webpack.config.js --env.TYPE=DIST",
    "bootstrap:hoist": "lerna bootstrap --hoist",
    "clean": "lerna clean --yes && rm -rf node_modules || true",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:build": "lerna ls | sed 's/@cerner\\///g' | cut -d ' ' -f1 | xargs -I {} babel packages/{}/src --out-dir packages/{}/lib --copy-files",
    "compile:clean": "rm -rf packages/**/lib",
    "compile:src": "lerna run compile",
    "compile:prod": "rm -rf ./build; NODE_ENV=production webpack -p",
    "compile:lowlight": "rm -rf ./build; THEME=clinical-lowlight-theme webpack -p",
    "compile:fusion": "rm -rf ./build; THEME=orion-fusion-theme webpack -p",
    "danger": "danger ci",
    "dev": "node packages/carbon-graphs/webpack/dev-server.js",
    "heroku-prebuild": "npx lerna init",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "link-parent-bin": "link-parent-bin",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint 'packages/**/src/**/*.scss' --syntax scss",
    "pretest": "npm run lint",
    "postinstall": "link-parent-bin && npm run compile && npm run bootstrap:hoist && npm run build && npm run build:dist",
    "start": "tt-serve",
    "start-heroku": "tt-serve-static --port $PORT --site './build' ",
    "start-prod": "tt-serve --env.disableHotReloading -p",
    "start-static": "npm run compile:prod && tt-serve-static --site ./build",
    "test": "npm run lint && npm run jest && npm run karma && npm run wdio",
    "karma": "karma start ./packages/carbon-graphs/karma.config.js --single-run --no-auto-watch",
    "version": "lerna version --no-git-tag-version && node ./scripts/changelog-updater/index.js",
    "wdio-default": "wdio",
    "wdio-lowlight": "NODE_OPTIONS=--max-old-space-size=2048 THEME=clinical-lowlight-theme wdio",
    "wdio-fusion": "THEME=orion-fusion-theme wdio",
    "wdio": "npm run wdio-default && npm run wdio-lowlight && npm run wdio-fusion"
  }
}
